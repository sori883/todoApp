{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "pwa-node",
        "name": "Launch via yarn",
        "request": "launch",
        "runtimeArgs": [
            "run",
            "start:dev"
        ],
        "runtimeExecutable": "yarn",
        "skipFiles": [
            "<node_internals>/**"
        ],
        "sourceMaps": true,
        "envFile": "${workspaceFolder}/packages/api/.env",
        "cwd": "${workspaceRoot}/packages/api",
        "console": "integratedTerminal",
      },
    ]
  }